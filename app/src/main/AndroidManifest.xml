<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eus.ehu.tta.graphiapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".userHomeActivity" />
        <activity android:name=".applicationHelpActivity" />
        <activity android:name=".drawerStudentActivity" />
        <activity android:name=".drawerTeacherActivity" />
        <activity android:name=".GameModesActivity" />
        <activity android:name=".IndividualModeActivity" />
        <activity android:name=".LevelsActivity" />
        <activity android:name=".Level1Activity" />
        <activity android:name=".Level2Activity" />
    </application>

</manifest>